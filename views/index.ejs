<!DOCTYPE html>
<html>
    <head>
        <meta charset="utf-8">
        <title>WHACK-A-THUG APP WITH MONGODB</title>
        <link rel="stylesheet" href="/css/style.css">
    </head>
        <%- include('../partials/header.ejs') %>
       
        <section> 
            <h1 class="showTitle">Whack-A-Thug <br> List of Moves</h1>
            <div class="container">   
            <!-- <ul> -->
                <% for(let i=0; i < moves.length; i++) { %>
                    <div class="column">
                        <!-- <li class="indexList">  -->
                            <iframe 
                            width="560" height="315" src="<%=moves[i].link%>" frameborder="0" >
                            </iframe>
                            <p>
                                <a class="movesClass" href="/moves/<%=moves[i].id; %>">
                                    <%=moves[i].name; %>
                                </a>
                            </p>
                    </div>
                <% } %>
            </div>
        </section>

        <%- include('../partials/footer.ejs') %>
    </body>
</html>